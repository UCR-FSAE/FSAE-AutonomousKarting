<?xml version="1.0"?>

 <root BTCPP_format="4" >
     <BehaviorTree ID="MainTree">
        <ReactiveSequence>
            <Action ID="LatPIDtuner" file_path="./src/launches/launches/config/carla/parking/carla_pid.json" outputs="{outputs}"/>
        </ReactiveSequence>
        <ReactiveSequence>
            <Condition ID="IfGoalReached" goal_radius="0.6" inputs="{inputs}"/>
            <Action ID="StopCar" inputs="{inputs}" outputs="{outputs}"/>
        </ReactiveSequence>
     </BehaviorTree>
 </root>